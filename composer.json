{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=7.0.8",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/admin_toolbar": "^2.0",
        "drupal/adminimal_admin_toolbar": "^1.10",
        "drupal/back_to_top": "^1.0",
        "drupal/block_class": "^1.0",
        "drupal/block_visibility_groups": "^1.3",
        "drupal/bootstrap": "^3.20",
        "drupal/captcha": "^1.0@beta",
        "drupal/console": "^1.0.2",
        "drupal/contact_emails": "^1.18",
        "drupal/contact_storage": "^1.0@beta",
        "drupal/contact_tools": "^1.0@RC",
        "drupal/core": "^8.7.0",
        "drupal/easy_breadcrumb": "^1.12",
        "drupal/entity_clone": "^1.0@beta",
        "drupal/eu_cookie_compliance": "^1.8",
        "drupal/external": "^1.0@alpha",
        "drupal/field_group": "^3.0",
        "drupal/imagemagick": "^2.6",
        "drupal/linkit": "^4.3",
        "drupal/maillog": "1.x-dev",
        "drupal/masquerade": "^2.0@beta",
        "drupal/metatag": "^1.10",
        "drupal/paragraphs": "^1.10",
        "drupal/pathauto": "^1.5",
        "drupal/photoswipe": "^2.9",
        "drupal/recaptcha": "^2.4",
        "drupal/redirect": "^1.4",
        "drupal/robotstxt": "^1.2",
        "drupal/scheduler": "^1.1",
        "drupal/search_api": "^1.14",
        "drupal/search_api_attachments": "^1.0@beta",
        "drupal/snippet_manager": "^1.7",
        "drupal/swiftmailer": "^1.0@beta",
        "drupal/token": "^1.5",
        "drupal/tvi": "^1.0@beta",
        "drupal/twig_tweak": "^2.4",
        "drupal/webform": "^5.5",
        "drupal/youtube": "^1.0@beta",
        "drush/drush": "^9.0.0",
        "symfony/filesystem": "3.4.x-dev",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.7.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        },
        "patches": {
            "drupal/tvi":{
                "Route enhancer": "https://www.drupal.org/files/issues/2018-08-17/route_enhancer-2993234-3.patch"
}
}
    }
}
